          "top": "80",
          "right": "20",
          "bottom": "80",
          "left": "20"
        }
      },
      "elements": [
        {
          "id": "hero-content",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "text_align": "center"
          },
          "elements": [
            {
              "id": "hero-heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "About Our Plumbing Company",
                "size": "h1",
                "align": "center",
                "color": "#333333"
              }
            },
            {
              "id": "hero-tagline",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p class='lead'>Professional plumbing services you can trust. Serving the community with excellence and reliability.</p>",
                "align": "center"
              }
            },
            {
              "id": "hero-contact",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Call (555) 123-4567",
                "link": {
                  "url": "tel:5551234567"
                },
                "size": "lg",
                "button_background_color": "#007cba",
                "button_text_color": "#ffffff"
              }
            }
          ]
