{
  "version": "1.0.0",
  "title": "140 Dogwood Site Template - Contact Page",
  "type": "page",
  "content": [
    {
      "id": "hero-section",
      "elType": "container",
      "settings": {
        "content_width": "full",
        "background_background": "classic",
        "background_color": "#007cba",
        "padding": {
          "unit": "px",
          "top": "80",
          "right": "20",
          "bottom": "80",
          "left": "20"
        }
      },
      "elements": [
        {
          "id": "hero-content",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "text_align": "center"
          },
          "elements": [
            {
              "id": "hero-heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Contact Us Today",
                "size": "h1",
                "align": "center",
                "color": "#ffffff"
              }
            },
            {
              "id": "hero-tagline",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p style='color: #ffffff; font-size: 20px;'>Premier plumbing service in your area and surrounding communities</p>",
                "align": "center"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "contact-info-section",
      "elType": "container",
      "settings": {
        "content_width": "full",
        "background_background": "classic",
        "background_color": "#f8f9fa",
        "padding": {
          "unit": "px",
          "top": "60",
          "right": "20",
          "bottom": "60",
          "left": "20"
        }
      },
      "elements": [
        {
          "id": "contact-info-grid",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "flex_direction": "row",
            "flex_gap": {
              "unit": "px",
              "size": 40
            }
          },
          "elements": [
            {
              "id": "phone-info",
              "elType": "widget",
              "widgetType": "icon-box",
              "settings": {
                "icon": {
                  "value": "fas fa-phone",
                  "library": "fa-solid"
                },
                "title_text": "Call Us Now",
                "description_text": "(555) 123-4567",
                "link": {
                  "url": "tel:5551234567"
                },
                "position": "top",
                "icon_color": "#007cba",
                "title_color": "#333333"
              }
            },
            {
              "id": "email-info",
              "elType": "widget",
              "widgetType": "icon-box",
              "settings": {
                "icon": {
                  "value": "fas fa-envelope",
                  "library": "fa-solid"
                },
                "title_text": "Email Us",
                "description_text": "info@yourplumber.com",
                "link": {
                  "url": "mailto:info@yourplumber.com"
                },
                "position": "top",
                "icon_color": "#007cba",
                "title_color": "#333333"
              }
            },
            {
              "id": "location-info",
              "elType": "widget",
              "widgetType": "icon-box",
              "settings": {
                "icon": {
                  "value": "fas fa-map-marker-alt",
                  "library": "fa-solid"
                },
                "title_text": "Service Area",
                "description_text": "Your City & Surrounding Areas",
                "position": "top",
                "icon_color": "#007cba",
                "title_color": "#333333"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "contact-form-section",
      "elType": "container",
      "settings": {
        "content_width": "full",
        "background_background": "classic",
        "background_color": "#ffffff",
        "padding": {
          "unit": "px",
          "top": "60",
          "right": "20",
          "bottom": "60",
          "left": "20"
        }
      },
      "elements": [
        {
          "id": "form-container",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "flex_direction": "row",
            "flex_gap": {
              "unit": "px",
              "size": 60
            }
          },
          "elements": [
            {
              "id": "form-content",
              "elType": "container",
              "settings": {
                "flex": "1"
              },
              "elements": [
                {
                  "id": "form-heading",
                  "elType": "widget",
                  "widgetType": "heading",
                  "settings": {
                    "title": "Request Free Estimate",
                    "size": "h2",
                    "color": "#333333"
                  }
                },
                {
                  "id": "form-description",
                  "elType": "widget",
                  "widgetType": "text-editor",
                  "settings": {
                    "editor": "<p>Fill out the form below and we'll get back to you as soon as possible to schedule your appointment or provide a free estimate for your plumbing needs.</p>"
                  }
                },
                {
                  "id": "contact-form",
                  "elType": "widget",
                  "widgetType": "form",
                  "settings": {
                    "form_name": "Contact Form",
                    "form_fields": [
                      {
                        "custom_id": "name",
                        "field_type": "text",
                        "field_label": "Full Name",
                        "placeholder": "Your Full Name",
                        "required": true,
                        "width": "100"
                      },
                      {
                        "custom_id": "phone",
                        "field_type": "tel",
                        "field_label": "Phone Number",
                        "placeholder": "Your Phone Number",
                        "required": true,
                        "width": "50"
                      },
                      {
                        "custom_id": "email",
                        "field_type": "email",
                        "field_label": "Email Address",
                        "placeholder": "Your Email Address",
                        "required": true,
                        "width": "50"
                      },
                      {
                        "custom_id": "service",
                        "field_type": "select",
                        "field_label": "Service Needed",
                        "field_options": "Emergency Repair\nDrain Cleaning\nWater Heater Service\nPipe Repair\nFaucet/Toilet Repair\nLeak Detection\nNew Installation\nOther",
                        "required": true,
                        "width": "100"
                      },
                      {
                        "custom_id": "address",
                        "field_type": "text",
                        "field_label": "Property Address",
                        "placeholder": "Address where service is needed",
                        "width": "100"
                      },
                      {
                        "custom_id": "message",
                        "field_type": "textarea",
                        "field_label": "Description of Problem",
                        "placeholder": "Please describe your plumbing issue in detail",
                        "rows": 5,
                        "width": "100"
                      }
                    ],
                    "submit_button_text": "Request Estimate",
                    "button_background_color": "#007cba",
                    "button_text_color": "#ffffff",
                    "button_size": "lg"
                  }
                }
              ]
            },
            {
              "id": "service-steps",
              "elType": "container",
              "settings": {
                "flex": "1"
              },
              "elements": [
                {
                  "id": "steps-heading",
                  "elType": "widget",
                  "widgetType": "heading",
                  "settings": {
                    "title": "How It Works",
                    "size": "h3",
                    "color": "#333333"
                  }
                },
                {
                  "id": "step-1",
                  "elType": "widget",
                  "widgetType": "icon-box",
                  "settings": {
                    "icon": {
                      "value": "fas fa-calendar-alt",
                      "library": "fa-solid"
                    },
                    "title_text": "1. Set Up Appointment",
                    "description_text": "Contact us by phone or form to schedule your service appointment at your convenience.",
                    "position": "left",
                    "icon_color": "#007cba",
                    "title_color": "#333333"
                  }
                },
                {
                  "id": "step-2",
                  "elType": "widget",
                  "widgetType": "icon-box",
                  "settings": {
                    "icon": {
                      "value": "fas fa-running",
                      "library": "fa-solid"
                    },
                    "title_text": "2. We Come ASAP",
                    "description_text": "Our professional plumbers arrive promptly at your scheduled time with all necessary tools.",
                    "position": "left",
                    "icon_color": "#007cba",
                    "title_color": "#333333"
                  }
                },
                {
                  "id": "step-3",
                  "elType": "widget",
                  "widgetType": "icon-box",
                  "settings": {
                    "icon": {
                      "value": "fas fa-wrench",
                      "library": "fa-solid"
                    },
                    "title_text": "3. Resolve Your Problem",
                    "description_text": "We diagnose the issue and provide efficient, long-lasting solutions to fix your plumbing problems.",
                    "position": "left",
                    "icon_color": "#007cba",
                    "title_color": "#333333"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "id": "business-hours-section",
      "elType": "container",
      "settings": {
        "content_width": "full",
        "background_background": "classic",
