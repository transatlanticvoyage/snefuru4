              "id": "final-cta-button",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Call (555) 123-4567 Now",
                "link": {
                  "url": "tel:5551234567"
                },
                "size": "xl",
                "button_background_color": "#ffffff",
                "button_text_color": "#007cba",
